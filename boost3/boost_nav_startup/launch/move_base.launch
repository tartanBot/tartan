<?xml version="1.0"?>
<launch>
  <!--<master auto="start"/>-->

  <!-- Run the map server -->
  <!--<node name="map_server" pkg="map_server" type="map_server" args="$(find my_map_package)/my_map.pgm my_map_resolution"/> -->

  <!--- Run AMCL -->
  <!--<include file="$(find amcl)/examples/<amcl_omni></amcl_omni>.launch" />-->

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <remap from="odom" to="/odometry/filtered_map"/>

    <rosparam file="$(find boost_nav_startup)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find boost_nav_startup)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find boost_nav_startup)/params/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find boost_nav_startup)/params/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find boost_nav_startup)/params/base_local_planner_params.yaml" command="load" />
  </node>
</launch>
